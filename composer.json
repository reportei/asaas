{
    "name": "reportei/asaas",
    "description": "Asaas.com PHP API Wrapper",
    "type": "library",
    "license": "MIT",
    "keywords": ["asaas", "api", "php", "reportei"],
    "homepage": "https://github.com/reportei/asaas",
    "support": {
        "issues": "https://github.com/reportei/asaas/issues",
        "source": "https://github.com/reportei/asaas"
    },
    "autoload": {
        "psr-4": {
            "Reportei\\Asaas\\": "src/",
            "Reportei\\Asaas\\Services\\": "src/services"
        }
    },
    "authors": [
        {
            "name": "Pedro Paulo",
            "email": "pedropaulosilvaf@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.22",
        "vlucas/phpdotenv": "^5.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
